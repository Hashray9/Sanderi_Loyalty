{
  "permissions": {
    "allow": [
      "WebSearch",
      "WebFetch(domain:www.legendapp.com)",
      "WebFetch(domain:docs.expo.dev)",
      "Bash(pnpm --filter @sanderi/mobile add:*)",
      "Bash(where:*)",
      "Bash(cmd /c \"npx pnpm --filter @sanderi/mobile add @legendapp/motion expo-haptics\")",
      "Bash(cmd /c \"corepack enable && pnpm --filter @sanderi/mobile add @legendapp/motion expo-haptics\")",
      "Bash(cmd /c \"pnpm --version 2>&1\")",
      "Bash(cmd /c \"where pnpm 2>&1 & pnpm --version 2>&1 & echo DONE\")",
      "Bash(powershell -Command:*)",
      "Bash(node -e:*)",
      "Bash(node --no-warnings --eval:*)"
    ]
  }
}
