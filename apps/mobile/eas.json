{
  "cli": {
    "version": ">= 16.32.0",
    "appVersionSource": "remote",
    "requireCommit": false
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "node": "22.11.0",
      "env": {
        "EXPO_USE_METRO_WORKSPACE_ROOT": "1"
      },
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug"
      },
      "cache": {
        "disabled": true
      },
      "prebuildCommand": "npm install -g pnpm && pnpm install --shamefully-hoist"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "prebuildCommand": "npm install -g pnpm && pnpm install --shamefully-hoist"
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      },
      "prebuildCommand": "npm install -g pnpm && pnpm install --shamefully-hoist"
    }
  },
  "submit": {
    "production": {}
  }
}
